{
  "name": "ai-assistant-cli",
  "description": "AI-powered CLI tool for generating commands, fixing errors, and reviewing code using Google Gemini",
  "version": "1.0.0",
  "author": "Fathur",
  "bin": {
    "aiCli": "./bin/run.js",
    "ai-cli": "./bin/run.js"
  },
  "bugs": {
    "url": "https://github.com/fathuur7/my-assistant-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fathuur7/my-assistant-cli.git"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@inquirer/prompts": "^7.9.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/fathuur7/my-assistant-cli#readme",
  "keywords": [
    "cli",
    "ai",
    "assistant",
    "gemini",
    "code-generation",
    "error-fixing",
    "code-review",
    "automation",
    "developer-tools",
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "aiCli",
    "dirname": "ai-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
